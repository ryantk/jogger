<div>
  <% memories.each_slice(2) do |row| %>
    <div class="row">
      <% row.each do |memory| %>
        <div class="col-xs-6 col-md-6">
          <div class="thumbnail center">
            <%= send(memory.tag, memory.file.path, class: "only-fill") %>
          </div>
        </div>
      <% end %>
    </div>
  <% end %>
</div>